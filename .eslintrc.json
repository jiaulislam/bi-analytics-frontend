{
  "ignorePatterns": [
    "./node_modules/",
    "/build/",
    "./next/",
    "./dist/"
  ],
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "react/no-children-prop": "off",
    "no-extra-semi": "off"
  }
}
